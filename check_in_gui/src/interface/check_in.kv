#:kivy 2.0.0
#:include interface/screens/login.kv

<CheckIn>:
    BoxLayout:
        orientation:'vertical'
        height: root.height
        width: root.width
        #cols: 3
        #rows: 2
        ScreenManager:
            id: screen_manager
            LoginScreen:
                #center: root.center
                id: login_screen
                name: "login"
                manager: "screen_manager"
            StudentDashboardScreen:
                id: student_dashboard_screen
                name: "student_dashboard"
                manager: "screen_manager"
            TeacherDashboardScreen:
                id: teacher_dashboard_screen
                name: "teacher_dashboard"
                manager: "screen_manager"
            CreateUserScreen:
                id: create_user_screen
                name: "create_user"
                manager: "screen_manager"
            AboutScreen:
                id: about_from_login_screen
                name: "about"
                manager: "screen_manager"
            SubmitCheckInScreen:
                id: submit_check_in_screen
                name: "submit_check_in"
                manager: "screen_manager"
            StudentStatsScreen:
                id: student_stats_screen
                name: "student_stats"
                manager: "screen_manager"
                manager: "screen_manager"
            TeacherStatsScreen:
                id: teacher_stats_screen
                name: "teacher_stats"
                manager: "screen_manager"

