#:kivy 2.0.0
<StudentStatsScreen>
    StudentStats:

<StudentStats>
    GridLayout:
        height: root.height
        width: root.width
        cols:1
        BoxLayout:
            GridLayout:
                cols:1  
                Button:
                    id:"back_to_student_dashboard_from_stats_button_id"
                    height: root.height/4
                    text:"back"
                    on_press:root.to_dashboard()
                
        StudentStatsBox:

<StudentStatsBox>:
    Button:
        text: "Check-in Pie"
        on_press: root.get_check_in_pie(self)
