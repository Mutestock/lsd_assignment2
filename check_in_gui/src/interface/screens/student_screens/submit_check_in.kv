#:kivy 2.0.0
<SubmitCheckInScreen>
    SubmitCheckIn:

<SubmitCheckIn>
    GridLayout:
        height: root.height
        width: root.width
        cols:1
        rows:4
        Label:
            id:"submit_check_in_label_id"
            text:"This is the submit check in screen"
            font_size:30
            center_x:root.width/2
            top: root.top-50
        BoxLayout:
            GridLayout:
                cols:1
                rows:4
                Button:
                    id:"back_to_student_dashboard_from_check_in_button_id"
                    text:"back"
                    on_press:root.to_dashboard()
