#:kivy 2.0.0
<GroupOverviewScreen>
    GroupOverview:

<GroupOverview>
    GridLayout:
        height: root.height
        width: root.width
        cols:1
        rows:4
        Label:
            text:"This is the group screen"
            font_size:30
            center_x:root.width/2
            top: root.top-50
        BoxLayout:
            GridLayout:
                cols:1
                rows:2
                Button:
                    text:"back"
                    on_press:root.to_dashboard()
                Button:
                    text:"Create Group"
                    on_press:root.to_create_group()

                    