#:kivy 2.0.0
<TeacherStatsScreen>
    TeacherStats:

<TeacherStats>
    GridLayout:
        height: root.height
        width: root.width
        cols:1
        rows:4
        BoxLayout:
            height: root.height/4
            GridLayout:
                cols:1
                rows:4
                Button:
                    id:"back_to_teacher_dashboard_from_stats_button_id"
                    text:"back"
                    on_press:root.to_dashboard()
        StudentGridTeacherStats:


<StudentGridTeacherStats>:
    cols: 1
    rows: 3
    Button:
        text:"sync"
        on_press: root.sync(self)


<PieButton>:
    text: "Bake Pie"
    on_press: root.bake_pie()